# Load the package
library(DataFrameBeautifier)
# Sample Data Frame
df <- data.frame(
Name  = c("Alice", "Bob", NA, "David"),
Score = c(95, NA, 88, 92),
Age   = c(23, 25, 22, 21)
)
# -----------------------------------
# 1. Pretty Types
# -----------------------------------
cat("Column Types:\n")
pretty_types(df)
# -----------------------------------
# 2. Highlight Missing Values (NA)
# -----------------------------------
cat("\nNA Highlight:\n")
highlight_na(df, color = "lightpink")
# -----------------------------------
# 3. Highlight Maximum Values
# -----------------------------------
cat("\nMax Highlight:\n")
highlight_max(df, color = "lightgreen")
# -----------------------------------
# 4. Highlight Minimum Values
# -----------------------------------
cat("\nMin Highlight:\n")
highlight_min(df, color = "lightblue")
# -----------------------------------
# 5. Full Beautify - Highlight NA + Max + Min
# -----------------------------------
cat("\nFull Beautification:\n")
beautify_df(
df,
highlight = c("na", "max", "min"),
colors = list(
na  = "mistyrose",
max = "palegreen",
min = "lightcyan"
)
)
